


<div class="form-group">
    <h3>Pending Report Based On Cell Member</h3>

    <div  ng-app="grievances" ng-controller="Gtype_report as vm">
        <form class="form-group" ng-submit="vm.GCMtype()">
            <div class="row pb-1">
                <div class="col-xs-12">
                    <label class="control-lable">From Date</label>
                </div>
                <div class="col-sm-7">
                    <input type="date" ng-model="from_date" name="from_date" class="form-control" required>
                </div>
            </div>
            <div class="row pb-1">
                <div class="col-xs-12">
                    <label class="control-lable">To Date</label>
                </div>
                <div class="col-sm-7">
                    <input type="date" ng-model="to_date" name="to_date" class="form-control"  required>
                </div>
            </div>
            <div class="form-group">
                <div class="row">
                    <div class="col-xs-12">
                       <label class="control-label">Grievance Cell Member</label>
                   </div>
                   <div class="col-sm-7">
                    <select class="form-control" ng-model="GCM" name="GCM" required="">
                        <option value="">Select</option>
                        <option ng-repeat="obj in members" value="{{obj.name}}">{{obj.name}}</option>
                    </select>
                </div>
            </div>
            </div>
            <div row col-sm-7>
                <button type="submit" class="btn btn-secondary" name="">
                    Create
                </button>
            </div>
        </form>
    </div>

