<!--Management members-->
<!--Admin-->
<div ng-app="grievances" ng-controller="Admin_mngmnt">
    <h3>Management Members List</h3>
    <div id="grievances" class="contenttable">
        <div class="table-responsive">
        <table class="table table-hover table-striped">
            <thead class="table-inverse">
                <tr>
                    <th>#</th>
                    <th>Name</th>
                    <th>Designation</th>
                    <th>Email ID</th>
                    <th>Mobile No.</th>
                    <th>Action</th>
                </tr> 
            </thead>
            <tr ng-repeat="member in members">
                <td>{{$index+1}}</td>
                <td>{{member.name}}</td>
                <td>{{member.designation}}</td>
                <td>{{member.emailid}}</td>
                <td>{{member.mobileno}}</td>

                <td>
                    <button ng-click="Deactivate(member.emailid)" class="btn btn-danger btn-sm">
                        Deactivate
                    </button>
                </td>
            </tr>
        </table>
    </div>
    </div>


    <div id="loading">
        <div class="loader">
        </div>
        
        <h3>Loading...</h3>
    </div>

</div>

<script type="text/javascript">
    $('#closebutton').click(function(){
        $('.detailbackground').fadeOut(500);
    });
</script>